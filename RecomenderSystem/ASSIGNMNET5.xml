<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="PUBLICATIONLIST">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="PUPLICATION">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="AUTHORS">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="1" name="AUTHOR">
                      <xsd:complexType>
                        <xsd:attribute name="name" type="xsd:string" use="required" />
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="YEAR" type="xsd:integer" />
              <xsd:element name="TITLE" type="xsd:string" />
              <xsd:element minOccurs="0" name="CONFERENCE">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="CTITLE" type="xsd:string" />
                    <xsd:element name="ABBREVIATION" type="xsd:string" />
                    <xsd:element name="CYEAR" type="xsd:integer" />
                    <xsd:element name="LOCATION">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="CITY" type="xsd:string" />
                          <xsd:element name="COUNTRY" type="xsd:string" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" name="BOOKCHAPTER">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="EDITORS">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element minOccurs="1" name="EDITOR">
                            <xsd:complexType>
                              <xsd:attribute name="name" type="xsd:string" use="required" />
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="BOOKTITLE" type="xsd:string" />
                    <xsd:element name="PUBLISHER" type="xsd:string" />
                    <xsd:element name="PAGES" type="xsd:string" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element minOccurs="0" name="JOURNAL">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="NAME" type="xsd:string" />
                    <xsd:element name="VOLUME" type="xsd:string" />
                    <xsd:element name="PAGE" type="xsd:string" />
                    <xsd:element name="INDEX" type="xsd:string" />
                    <xsd:element name="IMPACTFACTOR" type="xsd:decimal" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>